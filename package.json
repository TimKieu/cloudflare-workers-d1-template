{
  "name": "cloudflare-workers-d1-template",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "wrangler dev --local --persist --port 3000",
    "dev:staging": "wrangler dev --port 3000",
    "deploy": "wrangler publish",
    "db:reset": "rimraf .wrangler/state/d1/DB.sqlite3 && wrangler d1 execute d1-template --local --file ./src/infra/repository/d1/migrations/0000_init.sql",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --verbose"
  },
  "dependencies": {
    "hono": "^2.5.6",
    "nanoid": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@faker-js/faker": "^7.6.0",
    "@types/jest": "^29.2.3",
    "axios": "^0.27.2",
    "better-sqlite3": "^8.0.0",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.3.1",
    "jest-environment-miniflare": "^2.11.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3",
    "wrangler": "2.5.0"
  }
}
